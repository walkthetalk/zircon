library("zbi-bootfs") {
  sdk_headers = [ "zbi-bootfs/zbi-bootfs.h" ]
  sdk = "static"
  static = true
  kernel = true

  sources = [
    "zbi-bootfs.cc",
  ]
  deps = [
    "$zx/system/fidl/fuchsia-hardware-skipblock:c",
    "$zx/system/fidl/fuchsia-io:c",
    "$zx/system/ulib/bootdata",
    "$zx/system/ulib/bootfs",
    "$zx/system/ulib/fdio",
    "$zx/system/ulib/fzl",
  ]
}
