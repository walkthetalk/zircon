library("devmgr-launcher") {
  sdk = "shared"
  sdk_headers = [
    "lib/devmgr-launcher/launch.h",
    "lib/devmgr-launcher/processargs.h",
  ]
  shared = true
  sources = [
    "launcher.cc",
  ]
  public_deps = [
    "$zx/system/ulib/fit",
  ]
  deps = [
    "$zx/system/ulib/fbl",
    "$zx/system/ulib/fdio",
    "$zx/system/ulib/zircon",
    "$zx/system/ulib/zx",
  ]
  configs += [ "$zx/public/gn/config:static-libc++" ]
}
