group("misc") {
  deps = [
    "builtin",
    "console",
    "cpu-trace",
    "goldfish",
    "goldfish-address-space",
    "goldfish-control",
    "ktrace",
    "pty",
    "sysinfo",
    "test",
  ]

  if (current_cpu == "x64") {
    deps += [ "tpm" ]
  }
}
