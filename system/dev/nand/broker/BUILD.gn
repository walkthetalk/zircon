group("broker") {
  deps = [
    ":nand-broker",
  ]
}

driver("nand-broker") {
  sources = [
    "broker.cc",
  ]
  deps = [
    "$zx/system/banjo/ddk.protocol.nand",
    "$zx/system/fidl/fuchsia-hardware-nand:c",
    "$zx/system/fidl/fuchsia-nand:c",
    "$zx/system/ulib/ddk",
    "$zx/system/ulib/ddktl",
    "$zx/system/ulib/fbl",
    "$zx/system/ulib/sync",
    "$zx/system/ulib/zircon",
    "$zx/system/ulib/zx",
  ]
}
