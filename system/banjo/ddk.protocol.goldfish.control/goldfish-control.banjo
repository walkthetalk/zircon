library ddk.protocol.goldfish.control;

using zx;

[Layout = "ddk-protocol"]
protocol GoldfishControl {
    /// Get color buffer for VMO. Fails if VMO is not associated with a color
    /// buffer.
    GetColorBuffer(handle<vmo> vmo) -> (zx.status ret, uint32 cb);
};
