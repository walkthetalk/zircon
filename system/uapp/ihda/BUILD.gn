executable("ihda") {
  sources = [
    "ihda.cc",
    "intel_hda_codec.cc",
    "intel_hda_controller.cc",
    "intel_hda_device.cc",
    "print_codec_state.cc",
    "zircon_device.cc",
  ]
  deps = [
    "$zx/system/fidl/fuchsia-hardware-intel-hda:c",
    "$zx/system/ulib/fbl",
    "$zx/system/ulib/fdio",
    "$zx/system/ulib/fzl",
    "$zx/system/ulib/intel-hda",
    "$zx/system/ulib/zircon",
    "$zx/system/ulib/zx",
  ]
}
